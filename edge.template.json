{
    "content": {
        "modulesContent": {
            "$edgeAgent": {
                "properties.desired.modules.simTempSensor": {
                    "settings": {
                        "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
                        "createOptions": ""
                    },
                    "type": "docker",
                    "env": {
                        "MessageCount": {
                            "value": "-1"
                        }
                    },
                    "status": "running",
                    "restartPolicy": "always",
                    "version": "1.0"
                },
                "properties.desired.modules.streamAnalytics": {
                    "settings": {
                        "image": "__ASA_IMAGE_NAME__",
                        "createOptions": ""
                    },
                    "type": "docker",
                    "env": __ASA_ENV__,
                    "status": "running",
                    "restartPolicy": "always",
                    "version": "1.0"
                }
            },
            "$edgeHub": {
                "properties.desired.routes.simTempSensorToStreamAnalytics": "FROM /messages/modules/simTempSensor/* INTO BrokeredEndpoint(\"modules/streamAnalytics/inputs/__ASA_INPUT__\")",
                "properties.desired.routes.streamAnalyticsToIoTHub": "FROM /messages/modules/streamAnalytics/outputs/__ASA_UPSTREAM_OUTPUT__ INTO $upstream"
            },
            "SimulatedTemperatureSensor": {
                "properties.desired": {
                    "SendData": true,
                    "SendInterval": 5
                }
            },
            "streamAnalytics": {
                "properties.desired": __DESIRED_PROPERTIES__
            }
        }
    }
}